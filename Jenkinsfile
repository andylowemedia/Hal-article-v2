pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh '''#!/bin/bash

echo "Build script"'''
      }
    }
    stage('test') {
      steps {
        sh '''#!/bin/bash

echo "Test script"'''
      }
    }
    stage('deploy') {
      steps {
        sh '''#!/bin/bash

echo "deploy script"'''
      }
    }
  }
}